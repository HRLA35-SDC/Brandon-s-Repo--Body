  "scripts": {
    "start": "nodemon  ./server/index.js",
    "dev:react": "webpack -d --watch",
    "db:seed": "node --max-old-space-size=4096 database-mongoose/seed.js",
    "db:import": "mongoimport -d nykeproducts -c products --type json --file database-mongoose/data.json --jsonArray",
    "psql:import": "cat database-mongoose/data.csv | psql -h localhost -p 5432 nykeproducts -c \"COPY products (nikeID,names,gender,types,discountprice,prices,colorstyles,productpictures,weights,lasts,offsetmeasurement,styles,collections,sizes) FROM STDIN(FORMAT CSV, HEADER TRUE, DELIMITER ';');\"",
    "psql:connect": "node database-psql/index.js",
    "mongo:connect": "node database-mongoose/index.js",
    "generate": "node database-mongoose/generateData.js",
    "test": "artillery run server/testing.yml"
  },
  "dependencies": {
    "Faker": "0.7.2",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "colors": "^1.4.0",
    "cors": "2.8.5",
    "docker": "^1.0.0",
    "express": "4.17.1",
    "file-loader": "^6.0.0",
    "mongo": "^0.1.0",
